CREATE USER- CREATE USER IDENTIFIED BY PASSWORD;
ALTER USER- ALTER USER USERNAME IDENTIFIED BY PASSWORD;
DELETE USER- DROP USER USERNAME[ CASCODE];
CREATE NEW USER- CREATE USER 'NEWUSER'@'LOCALHOST' IDENTIFIED BY 'PASSWORD';
GRANT ACCESS- GRANT ALL PRIVILIGES ON ** TO NEWUSER @ LOCALHOST;
GRANT PRIVILEGES- GRANT[TYPE OF PERMISSION]ON[DATABASE NAME],[TABLENAME]TO [USERNAME]@ LOCALHOST;
REVOKE PRIVILEGES- REVOKE[TYPE OF PERMISSION]ON[DATABASE NAME],[TABLENAME] FROM [USERNAME]@ LOCALHOST;
GRANT COMMAND- GRANT SELECT,INSERT,UPDATE,DELETE ON TABLENAME TO USERNAME;
REVOKE COMMAND- REVOKE SELECT,INSERT,UPDATE,DELETE ON TABLENAME FROM USERNAME;
